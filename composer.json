{
    "name": "danielkulbe/siteaddress",
    "description": "Display address and contact information your website.",
    "type": "bolt-extension",
    "keywords": [
        "address", "contact", "info", "phone", "owner"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Kulbe",
            "email": "info@daniel-kulbe.de"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git@bitbucket.org:danielkulbe/bolt-siteaddress.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:DanielKulbe/SiteAddress.git"
        }
    ],
    "require": {
        "bolt/bolt": ">=2.2.0,<3.0.0"
    },
    "autoload": {
        "files": [
            "init.php"
        ],
        "psr-4": {
            "Bolt\\Extension\\DanielKulbe\\SiteAddress\\": ""
        }
    },
    "extra": {
        "bolt-assets": "assets",
        "bolt-icon": "assets/address-symbol.png",
        "branch-alias" : {
            "dev-master" : "1.0.x-dev"
        }
    }
}